{
  "name": "han-excel-builder",
  "version": "1.0.0",
  "description": "Advanced Excel file generator with TypeScript support, comprehensive styling, and optimized performance",
  "type": "module",
  "main": "dist/han-excel-builder.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/han-excel-builder.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.js",
      "types": "./dist/styles.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "docs": "typedoc src/index.ts --out docs",
    "clean": "rimraf dist docs",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "excel",
    "xlsx",
    "typescript",
    "exceljs",
    "report",
    "spreadsheet",
    "workbook",
    "worksheet"
  ],
  "author": "Han Excel Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/han-excel-builder.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/han-excel-builder/issues"
  },
  "homepage": "https://github.com/your-org/han-excel-builder#readme",
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "terser": "^5.43.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.2",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5"
  },
  "peerDependencies": {
    "exceljs": "^4.4.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
